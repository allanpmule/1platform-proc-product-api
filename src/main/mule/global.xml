<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns:product-apifor-my-sqldb="http://www.mulesoft.org/schema/mule/product-apifor-my-sqldb"
	xmlns:tls="http://www.mulesoft.org/schema/mule/tls"
